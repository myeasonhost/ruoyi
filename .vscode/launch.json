    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Launch Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "Launch RuoYiApplication",
            "request": "launch",
            "mainClass": "com.dadsunion.RuoYiApplication",
            "projectName": "tron-admin",
            "console": "internalConsole",
            "vmArgs": "-Dspring.profiles.active=dev"
        },
        {
            "type": "java",
            "name": "Launch EscapeUtil",
            "request": "launch",
            "mainClass": "com.dadsunion.common.utils.html.EscapeUtil",
            "projectName": "tron-common"
        }
    ]
}